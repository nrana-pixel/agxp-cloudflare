# Environment Variables Example
# Copy this file to .env for local development

# Cloudflare Worker Secrets (set via wrangler secret put)
# ENCRYPTION_KEY=<base64-encoded-256-bit-key>
# JWT_SECRET=<your-jwt-secret>

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# API Base URL (used by client workers to send analytics)
API_BASE_URL=http://localhost:8787

# External integrations
FIRECRAWL_API_KEY=
GEMINI_API_KEY=

# Database Configuration
# These are configured in wrangler.toml, not .env
# DATABASE_ID=<your-d1-database-id>
# KV_SESSIONS_ID=<your-kv-sessions-namespace-id>
# KV_TOKENS_ID=<your-kv-tokens-namespace-id>
