{
    "name": "cloudflare-edge-backend",
    "version": "1.0.0",
    "description": "Cloudflare Edge Integration for AI-Optimized Variants",
    "main": "src/index.ts",
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "db:migrate": "wrangler d1 execute DB --file=./migrations/0001_initial_schema.sql",
        "db:migrate:local": "wrangler d1 execute DB --local --file=./migrations/0001_initial_schema.sql"
    },
    "keywords": [
        "cloudflare",
        "workers",
        "hono",
        "edge",
        "ai"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "hono": "^4.0.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240208.0",
        "typescript": "^5.3.3",
        "vitest": "^1.2.2",
        "wrangler": "^3.28.0"
    }
}